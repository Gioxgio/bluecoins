sqlite3-editor-win32-x64.exe export --database-filepath bluecoins.fydb --format csv --query "SELECT     substr(date, 0, 5) as ""Year"",     act.accountName ""Account"",     replace(amount * 1. / 1000000, '.', ',') as ""Amount"",     ttt.transactionTypeName as ""Type"",     pct.parentCategoryName as ""Category"" FROM     TRANSACTIONSTABLE tt     inner join ACCOUNTSTABLE act on act.accountsTableID = tt.accountID     inner join TRANSACTIONTYPETABLE ttt on ttt.transactionTypeTableID = tt.transactionTypeID     inner join CHILDCATEGORYTABLE cct on cct.categoryTableID = tt.categoryID     inner join PARENTCATEGORYTABLE pct on pct.parentCategoryTableID = cct.parentCategoryID where     deletedTransaction = 6" --output-file out.csv